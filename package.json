    "aws-sdk-mock": "^4.5.0",
    "chai": "^4.2.0",
    "codelyzer": "^5.1.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "faker": "^4.1.0",
    "husky": "^4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^10.4.0",
    "mocha": "^8.1.3",
