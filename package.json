    "@angular/cli": "~9.1.9",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "~10.1.5",
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@lhci/cli": "^0.4.1",
