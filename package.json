    "protractor": "^7.0.0",
    "puppeteer": "^2.1.1",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "sandboxed-module": "^2.0.4",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2",
    "ts-node": "~7.0.1",
    "tslint": "~6.1.3",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": "12.19.0",
    "npm": "6.14.8"
  },
  "nyc": {
    "branches": 10,
    "lines": 15,
    "functions": 15,
    "statements": 15,
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "include": [
      "server/**/*.js",
      "server.js"
    ],
    "exclude": [
      "server/test/**/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,component.html}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
