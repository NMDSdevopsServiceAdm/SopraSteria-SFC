{
  "name": "ng-sfc-v2",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "node --max-old-space-size=4096 server.js",
    "dev": "npm-run-all -p -l build:watch api:proxy",
    "build": "ng build  --extract-css",
    "build:clean": "rimraf dist",
    "build:watch": "ng build --extract-css --watch --no-delete-output-path",
    "build:prod": "npm run build:clean && ng build --extract-css --prod",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:coverage": "c8 node server.js",
    "metrics": "opener http://localhost:3000/apimetrics/ui",
    "api:proxy": "node proxy.js",
    "api:server": "node server.js",
    "server": "npm-run-all -p -l build:watch api:server",
    "server:test": "mocha server/test/**/*.spec.js",
    "server:test:unit": "mocha --exit server/test/unit/**/*.spec.js",
    "server:test:integration": "mocha server/test/integration/**/*.spec.js",
    "server:cover": "nyc --reporter=html mocha server/test/**/*.spec.js",
    "server:cover:unit": "nyc --reporter=html mocha --exit 'server/test/unit/**/*.spec.js'",
    "server:cover:integration": "nyc --reporter=html mocha server/test/integration/**/*.spec.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/forms": "~9.1.7",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@auth0/angular-jwt": "^4.1.2",
    "ajv": "^6.12.2",
    "angulartics2": "^9.0.1",
    "aws-sdk": "^2.678.0",
    "axios": "^0.19.2",
    "bcrypt-nodejs": "0.0.3",
    "c8": "^7.1.2",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.2.0",
    "convict": "^6.0.0",
    "convict-format-with-validator": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "csvtojson": "^2.0.10",
    "details-element-polyfill": "^2.4.0",
    "express-http-proxy": "^1.6.0",
    "express-sanitizer": "^1.0.5",
    "file-saver": "^2.0.2",
    "govuk-frontend": "^2.13.0",
    "helmet": "^3.22.0",
    "is-windows": "^1.0.2",
    "js-yaml": "^3.13.1",
    "jsdom": "^16.2.2",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "ngx-moment": "^3.5.0",
    "notifications-node-client": "^4.7.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.4",
    "rate-limiter-flexible": "^2.1.4",
    "rfr": "^1.2.3",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "sequelize": "^4.44.3",
    "serve-favicon": "^2.5.0",
    "slack": "^11.0.2",
    "slugify": "^1.4.0",
    "swagger-stats": "^0.95.17",
    "ua-parser-js": "^0.7.21",
    "url-parse": "^1.4.7",
    "util": "^0.12.3",
    "uuid": "^3.3.3",
    "walk": "^2.3.14",
    "xss-clean": "^0.1.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "^9.1.7",
    "@angular/language-service": "~9.1.7",
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@testing-library/angular": "^9.2.2",
    "@types/faker": "^4.1.12",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.0.1",
    "aws-sdk-mock": "^5.1.0",
    "chai": "^4.2.0",
    "codelyzer": "^5.2.2",
    "cross-env": "^7.0.2",
    "faker": "^4.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.0.7",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "mocha": "^7.1.2",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.4",
    "npm": "^6.14.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "opener": "^1.5.1",
    "protractor": "~7.0.0",
    "puppeteer": "^3.0.4",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "sandboxed-module": "^2.0.4",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3"
  },
  "engines": {
    "node": "10.19.0",
    "npm": "6.13.4"
  },
  "nyc": {
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "include": [
      "server/**/*.js",
      "server.js"
    ],
    "exclude": [
      "server/test/**/*"
    ]
  }
}
