{
  "name": "ng-sfc-v2",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && node --max-old-space-size=4096 server.js",
    "dev": "npm-run-all -p -l build:watch api:proxy",
    "build": "ng build  --extract-css",
    "build:clean": "rimraf dist",
    "build:watch": "ng build --extract-css --watch --no-delete-output-path",
    "build:prod": "npm run build:clean && ng build --extract-css --prod",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:coverage": "c8 node server.js",
    "metrics": "opener http://localhost:3000/apimetrics/ui",
    "api:proxy": "node proxy.js",
    "api:server": "node server.js",
    "server": "npm-run-all -p -l build:watch api:server",
    "server:test": "mocha server/test/**/*.spec.js",
    "server:test:unit": "mocha --exit server/test/unit/**/*.spec.js",
    "server:test:integration": "mocha server/test/integration/**/*.spec.js",
    "server:cover": "nyc --reporter=html mocha server/test/**/*.spec.js",
    "server:cover:unit": "nyc --reporter=html mocha --exit server/test/unit/**/*.spec.js",
    "server:cover:integration": "nyc --reporter=html mocha server/test/integration/**/*.spec.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "@auth0/angular-jwt": "^3.0.0",
    "ajv": "^6.10.2",
    "angulartics2": "^7.4.1",
    "aws-sdk": "^2.515.0",
    "axios": "^0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "c8": "^3.2.1",
    "concurrently": "^4.1.2",
    "convict": "^4.4.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.4",
    "csvtojson": "^2.0.8",
    "details-element-polyfill": "^2.4.0",
    "express-http-proxy": "^1.5.1",
    "express-sanitizer": "^1.0.5",
    "file-saver": "^2.0.2",
    "govuk-frontend": "^2.13.0",
    "helmet": "^3.15.0",
    "is-windows": "^1.0.2",
    "jade": "^1.11.0",
    "js-yaml": "^3.12.2",
    "jsdom": "^15.1.1",
    "jsonwebtoken": "^8.4.0",
    "jszip": "^3.2.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "ngx-moment": "^3.4.0",
    "notifications-node-client": "^4.6.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.2",
    "rfr": "^1.2.3",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "sequelize": "^4.44.3",
    "serve-favicon": "^2.5.0",
    "slack": "^11.0.1",
    "slugify": "^1.3.4",
    "swagger-stats": "^0.95.7",
    "ua-parser-js": "^0.7.20",
    "url-parse": "^1.4.7",
    "util": "^0.11.1",
    "uuid": "^3.3.3",
    "walk": "^2.3.14",
    "xss-clean": "^0.1.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.2",
    "@angular/cli": "~8.2.2",
    "@angular/compiler-cli": "^8.2.3",
    "@angular/language-service": "~8.2.3",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "chai": "^4.2.0",
    "codelyzer": "^5.0.0",
    "cross-env": "^5.2.1",
    "faker": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^6.2.2",
    "nodemon": "^1.18.9",
    "npm": "^6.11.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "opener": "^1.5.1",
    "protractor": "~5.4.0",
    "puppeteer": "^2.0.0",
    "require-dir": "^1.2.0",
    "rimraf": "^2.7.1",
    "sandboxed-module": "^2.0.3",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  },
  "engines": {
    "node": "10.10.0",
    "npm": "6.4.1"
  },
  "nyc": {
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "include": [
      "server/**/*.js",
      "server.js"
    ],
    "exclude": [
      "server/test/**/*"
    ]
  }
}
